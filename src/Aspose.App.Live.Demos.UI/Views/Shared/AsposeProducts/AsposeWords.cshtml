@model LandingPageModel

	<div class="container-fluid header1 header2 minify-header appfamily-header">
		<div class="container">
			<div class="row">
				<div class="col-md-8">
					<h1>@Model.Resources["WordsPageMainTitle"] </h1>
					<h2>@Model.Resources["WordsPageSubHeading"]  </h2>
				</div>
				<div class="col-md-4 tc">
					<img class=" col-xs-12" src="../img/aspose-words-app.png" alt="Aspose.Words" width="320" />
				</div>
				<!--/col6 -->
			</div>
			<!--/row -->
		</div>
		<!--/container -->
	</div>
	<div class="container-fluid productfamilypage bg-white pdb-5">
		<div class="row">
			<div class="container tc">
				<div class="clearfix">&nbsp;</div>
				<div class="clearfix">&nbsp;</div>
				<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeWords"]) </h3>
				<div class="clearfix">&nbsp;</div>
				<div class="clearfix">&nbsp;</div>


				<div class="col-lg-4 productfamily_box">
					<div class="productfamilytitle">
						<div class="imgblock">
							<img src="../img/aspose_words-for-net.png" alt="Aspose.Words for .NET" />
						</div>
						<a href="/words/conversion"><span class="spanclass">@Model.Resources["AsposeWords"] @Model.Resources["AsposeProductFor"]<em>@Model.Resources["Conversionh4"]</em></span><em>@Model.Resources["wordsConversionDesc"]</em></a>
					</div>

				</div>
			</div>
		</div>
	</div>
	<div class="spacer">&nbsp;</div>

