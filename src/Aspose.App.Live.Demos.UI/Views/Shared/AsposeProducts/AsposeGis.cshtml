@model LandingPageModel

<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>@Model.Resources["GisPageMainTitle"]</h1>
				<h4>@Model.Resources["GisPageSubHeading"]</h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose-gis-app.png" alt="Aspose.Gis" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeGis"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_gis-for-net.png" alt="Aspose.Gis for .NET" />
					</div>
					<a href="/gis/conversion"><span class="spanclass">@Model.Resources["AsposeGis"] @Model.Resources["AsposeProductFor"]  <em>Conversion</em></span><em>@Model.Resources["GisConversionDesc"] </em></a>
				</div>
			</div>

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>