@model LandingPageModel

<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>@Model.Resources["3dPageMainTitle"]</h1>
				<h4>@Model.Resources["3dPageSubHeading"] </h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose-3d-app.png" alt="Aspose.3d" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["Aspose3D"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_3d-for-net.png" alt="Aspose.3d for .NET" />
					</div>
					<a href="/3d/conversion"><span class="spanclass">@Model.Resources["Aspose3D"] @Model.Resources["AsposeProductFor"] <em>Conversion</em></span><em>@Model.Resources["3dConversionDesc"]</em></a>
				</div>
			</div>

		</div>	</div>
</div>
<div class="spacer">&nbsp;</div>


