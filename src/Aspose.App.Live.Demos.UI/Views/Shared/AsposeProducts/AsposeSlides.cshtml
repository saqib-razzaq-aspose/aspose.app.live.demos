@model LandingPageModel

<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>@Model.Resources["SlidesPageMainTitle"] </h1>
				<h4>@Model.Resources["SlidesPageSubHeading"] </h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose-slides-app.png" alt="Aspose.Eps" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeSlides"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_slides-for-net-min.png" alt="Aspose.PSD for .NET" />
					</div>
					<a href="/slides/conversion"><span class="spanclass">@Model.Resources["AsposeSlides"] @Model.Resources["AsposeProductFor"]  <em>Conversion</em></span><em>@Model.Resources["SlidesConversionDesc"] </em></a>
				</div>
			</div>



		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>
